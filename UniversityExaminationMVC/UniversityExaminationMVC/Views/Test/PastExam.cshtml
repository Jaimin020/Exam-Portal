@using UniversityExaminationMVC.Models
@{
    ViewBag.Title = "PastExam";
    Layout = "~/Views/Shared/_LayoutPage_Student.cshtml";
}


@ViewBag.Date

<h2>Past Papers</h2>

<table class="table table-striped">
    <thead>
        <tr>
            <th scope="col">Date</th>
            <th scope="col">Name</th>
            <th scope="col">Subject</th>
            <th scope="col"></th>
        </tr>
    </thead>
    <tbody>

        @foreach (Paper ex in ViewBag.upPapers)
        {
            <tr>
                <th scope="row">@ex.paperDate</th>
                <td>@ex.Name</td>
                <td>@ex.Subject.Name</td>
                <td>
                    <a href="~/Test/ViewResult/@ex.Id"class="btn btn-dark">
                        ViewResult
                    </a>
                </td>
            </tr>

        }

    </tbody>
</table>


